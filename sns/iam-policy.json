{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "SNSContactFormPermissions",
      "Effect": "Allow",
      "Action": [
        "sns:CreateTopic",
        "sns:DeleteTopic",
        "sns:GetTopicAttributes",
        "sns:SetTopicAttributes",
        "sns:Subscribe",
        "sns:Unsubscribe",
        "sns:Publish",
        "sns:ListTopics",
        "sns:ListSubscriptions",
        "sns:ListSubscriptionsByTopic"
      ],
      "Resource": [
        "arn:aws:sns:*:ACCOUNT-ID:contact-form-*",
        "arn:aws:sns:*:ACCOUNT-ID:*"
      ]
    }
  ]
}
